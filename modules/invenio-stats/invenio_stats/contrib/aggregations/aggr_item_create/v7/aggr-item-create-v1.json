{
  "index_patterns": ["__SEARCH_INDEX_PREFIX__stats-item-create-*"],
  "settings": {
    "index": {
      "refresh_interval": "1m",
      "lifecycle.name": "weko_stats_policy",
      "lifecycle.rollover_alias": "__SEARCH_INDEX_PREFIX__stats-item-create"
    }
  },
  "mappings": {
      "date_detection": false,
      "dynamic": false,
      "numeric_detection": false,
      "properties": {
        "timestamp": {
          "type": "date",
          "format": "date_optional_time"
        },
        "unique_id": {
          "type": "keyword",
          "index": true
        },
        "count": {
          "type": "integer"
        },
        "unique_count": {
          "type": "integer"
        },
        "country": {
          "type": "keyword",
          "index": true
        },
        "cur_user_id": {
          "type": "keyword",
          "index": true
        },
        "hostname": {
          "type": "keyword",
          "index": true
        },
        "remote_addr": {
          "type": "keyword",
          "index": true
        },
        "pid_value": {
          "type": "keyword",
          "index": true
        },
        "record_name": {
          "type": "keyword",
          "index": true
        },
        "collection": {
          "type": "keyword",
          "index": true
        }
      }
    }
  }
