{
  "index_patterns": ["__SEARCH_INDEX_PREFIX__events-stats-search-*"],
  "settings": {
    "index": {
      "refresh_interval": "5s",
      "lifecycle.name": "weko_stats_policy",
      "lifecycle.rollover_alias": "__SEARCH_INDEX_PREFIX__events-stats-search"
    }
  },
  "mappings": {
      "date_detection": false,
      "dynamic": false,
      "numeric_detection": false,
      "properties": {
        "timestamp": {
          "type": "date",
          "format": "strict_date_hour_minute_second"
        },
        "labels": {
          "type": "keyword",
          "index": true
        },
        "search_detail": {
          "properties": {
            "search_key": {
              "type": "keyword",
              "index": true
            },
            "search_type": {
              "type": "keyword",
              "index": true
            }
          }
        },
        "unique_id": {
          "type": "keyword",
          "index": true
        },
        "country": {
          "type": "keyword",
          "index": true
        },
        "visitor_id": {
          "type": "keyword",
          "index": true
        },
        "unique_session_id": {
          "type": "keyword",
          "index": true
        },
        "referrer": {
          "type": "keyword",
          "index": true
        },
        "is_robot": {
          "type": "boolean"
        },
        "is_restricted": {
          "type": "boolean"
        },
        "site_license_name": {
          "type": "keyword",
          "index": true
        },
        "site_license_flag": {
          "type": "boolean"
        }
      }
    }
  }
