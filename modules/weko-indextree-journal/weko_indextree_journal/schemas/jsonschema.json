      "title": "Title alternative"
    },
    "title_transcription": {
      "type": "string",
      "title": "Title transcription"
    },
    "ncid": {
      "type": "string",
      "title": "NCID",
      "maxLength": 10,
      "pattern": "^[A-Z]{2}[0-9]{7}[0-9X]$",
      "validationMessage_i18n": {
        "202": {
          "en": "NCID format is wrong",
          "ja": "「NCID」のフォーマットが正しくない。"
