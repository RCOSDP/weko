{
  "settings": {
    "index.mapper.dynamic": false,
    "index": {
      "refresh_interval": "5s",
      "lifecycle.name": "weko_stats_policy"
    }
  },
  "mappings": {
    "stats-record-view": {
      "date_detection": false,
      "dynamic": false,
      "numeric_detection": false,
      "properties": {
        "timestamp": {
          "type": "date",
          "format": "strict_date_hour_minute_second"
        },
        "unique_id": {
          "type": "keyword",
          "index": true
        },
        "record_id": {
          "type": "keyword",
          "index": true
        },
        "record_name": {
          "type": "keyword",
          "index": true
        },
        "pid_type": {
          "type": "keyword",
          "index": true
        },
        "pid_value": {
          "type": "keyword",
          "index": true
        },
        "labels": {
          "type": "keyword",
          "index": true
        },
        "country": {
          "type": "keyword",
          "index": true
        },
        "visitor_id": {
          "type": "keyword",
          "index": true
        },
        "unique_session_id": {
          "type": "keyword",
          "index": true
        },
        "referrer": {
          "type": "keyword",
          "index": true
        },
        "is_robot": {
          "type": "keyword",
          "index": true
        },
        "is_restricted": {
          "type": "keyword",
          "index": true
        },
        "hostname": {
          "type": "keyword",
          "index": true
        },
        "remote_addr": {
          "type": "keyword",
          "index": true
        },
        "cur_user_id": {
          "type": "keyword",
          "index": true
        },
        "record_index_list": {
          "type": "nested",
          "properties": {
            "index_id": {
              "type": "keyword"
            },
            "index_name_en": {
              "type": "keyword",
              "index": true
            },
            "index_name": {
              "type": "keyword",
              "index": true
            }
          }
        },
        "site_license_name": {
          "type": "keyword",
          "index": true
        },
        "site_license_flag": {
          "type": "boolean"
        }
      }
    }
  }
}
